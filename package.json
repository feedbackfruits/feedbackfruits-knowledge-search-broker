    "ava": "^0.20.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "blue-tape": "^0.2.0",
    "codecov": "^1.0.1",
    "dataloader": "^1.3.0",
    "nock": "^9.0.14",
    "nodemon": "^1.11.0",
    "nyc": "^11.1.0",
    "supertest": "^3.0.0",
    "tap": "^5.7.0"

  "description": "",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "migrate": "east migrate",
    "clean": "rm -rf lib",
    "prestart": "yarn run build",
    "start": "node lib",
    "pretest": "yarn run clean && tsc --inlineSourceMap --outDir lib",
    "test": "nyc ava --verbose",
    "posttest": "yarn run clean && yarn run build",
    "watch-test": "nodemon -w test -x 'yarn run test'",
    "coverage": "codecov",
    "watch": "nodemon lib"
  },
  "nyc": {},
  "ava": {
    "files": [
      "test/**/*.js"
    ],
    "source": [
      "src/**/*.ts"
    ],
    "match": [],
    "concurrency": 5,
    "failFast": true,
    "failWithoutAssertions": false,
    "tap": false,
    "powerAssert": false,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "author": "Joost Verdoorn",
  "license": "MIT",
  "dependencies": {
    "@reactivex/rxjs": "^5.4.2",
    "@types/node": "^8.0.20",
    "@types/node-fetch": "^1.6.7",
    "dotenv": "^2.0.0",
    "east": "^1.0.0",
    "east-elasticsearch": "^1.0.1",
    "elasticsearch": "^12.1.3",
    "feedbackfruits-knowledge-engine": "github:feedbackfruits/feedbackfruits-knowledge-engine#^1.4.1",
    "git-repo-version": "^1.0.2",
    "memux": "github:knowledge-express/memux#~0.4.2",
    "node-fetch": "^1.6.3",
    "p-debounce": "^1.0.0",
    "p-map": "^1.1.1",
